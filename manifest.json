{
  "manifest_version": 2,
  "key": "",
  "name": "change DOM",
  "version": "1.0",
  "description": "Change some DOM element",

  "browser_action": {
    "default_icon": "images/get_started16.png"
  },

  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["https://www.flipkart.com/*"],
      "all_frames": true,
      "js": ["inject.js"],
      "css":["style.css"]
    }
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+Shift+P",
        "linux": "Ctrl+Shift+K",
        "windows": "Alt+Shift+P",
        "mac": "Alt+Shift+A"
      }
    }
  },
  "permissions": ["activeTab", "webNavigation", "*://*/*"]
}
